    %jmp T_0;
    .thread T_0;
    .scope S_0x18685b0;
T_1 ;
    %vpi_call 4 19 "$readmemh", "mem.dat", v0x1897c80 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x18688f0;
T_2 ;
    %vpi_call 2 28 "$display", "Starting instruction test.." {0 0 0};
    %pushi/vec4 268435457, 0, 32;
    %store/vec4 v0x1899840_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x18990c0_0, 0, 32;
    %delay 1000, 0;
    %load/vec4 v0x1898ef0_0;
    %pushi/vec4 268435457, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x18992f0_0;
    %load/vec4 v0x1899840_0;
    %parti/s 5, 16, 6;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %vpi_call 2 41 "$display", "Test 1 Passed Instuct: %b RT: %b  test1: %b", v0x1898ef0_0, v0x18992f0_0, v0x1899840_0 {0 0 0};
    %jmp T_2.1;
T_2.0 ;
    %vpi_call 2 44 "$display", "Test 1 Failed Instruct: %b RT: %b  test1: %b", v0x1898ef0_0, v0x18992f0_0, v0x1899840_0 {0 0 0};
T_2.1 ;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x18990c0_0, 0, 32;
    %load/vec4 v0x1898ef0_0;
    %cmpi/e 268435471, 0, 32;
    %jmp/0xz  T_2.2, 4;
    %vpi_call 2 49 "$display", "Test 2 Passed" {0 0 0};
    %jmp T_2.3;
T_2.2 ;
    %vpi_call 2 52 "$display", "Test 2 Failed" {0 0 0};
T_2.3 ;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x18990c0_0, 0, 32;
    %load/vec4 v0x1898ef0_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_2.4, 4;
    %vpi_call 2 57 "$display", "Test 3 Passed" {0 0 0};
    %jmp T_2.5;
T_2.4 ;
    %vpi_call 2 60 "$display", "Test 3 Failed" {0 0 0};
T_2.5 ;
    %end;
    .thread T_2;
