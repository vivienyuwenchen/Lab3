T_0 ;
    %wait E_0x1867650;
    %load/vec4 v0x1897d40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x18978c0_0;
    %ix/getv 3, v0x1867530_0;
