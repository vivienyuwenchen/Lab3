#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-107-gab6ae79)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1136b00 .scope module, "testMemory" "testMemory" 2 3;
 .timescale 0 0;
v0x116f9c0_0 .var "Addr", 31 0;
v0x116faa0_0 .var "DataIn", 31 0;
v0x116fb70_0 .net "DataOut", 31 0, L_0x116ff20;  1 drivers
v0x116fc70_0 .var "clk", 0 0;
v0x116fd40_0 .var "regWE", 0 0;
S_0x1139e60 .scope module, "Instruction_Memory" "memory_test" 2 10, 3 1 0, S_0x1136b00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "regWE"
    .port_info 2 /INPUT 32 "Addr"
    .port_info 3 /INPUT 32 "DataIn"
    .port_info 4 /OUTPUT 32 "DataOut"
L_0x116ff20 .functor BUFZ 32, L_0x116fe30, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x113a0b0_0 .net "Addr", 31 0, v0x116f9c0_0;  1 drivers
v0x116f3e0_0 .net "DataIn", 31 0, v0x116faa0_0;  1 drivers
v0x116f4c0_0 .net "DataOut", 31 0, L_0x116ff20;  alias, 1 drivers
v0x116f5b0_0 .net *"_s0", 31 0, L_0x116fe30;  1 drivers
v0x116f690_0 .net "clk", 0 0, v0x116fc70_0;  1 drivers
v0x116f7a0 .array "mem", 0 1023, 31 0;
v0x116f860_0 .net "regWE", 0 0, v0x116fd40_0;  1 drivers
E_0x1137190 .event posedge, v0x116f690_0;
L_0x116fe30 .array/port v0x116f7a0, v0x116f9c0_0;
    .scope S_0x1139e60;
T_0 ;
    %wait E_0x1137190;
    %load/vec4 v0x116f860_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x116f3e0_0;
    %ix/getv 3, v0x113a0b0_0;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x116f7a0, 0, 4;
T_0.0 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1139e60;
T_1 ;
    %vpi_call 3 19 "$readmemh", "mem.dat", v0x116f7a0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x1136b00;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x116f9c0_0, 0, 32;
    %load/vec4 v0x116fb70_0;
    %cmpi/e 268435457, 0, 32;
    %jmp/0xz  T_2.0, 4;
    %vpi_call 2 15 "$display", "Test 1 Passed" {0 0 0};
    %jmp T_2.1;
T_2.0 ;
    %vpi_call 2 18 "$display", "Test 1 Failed" {0 0 0};
T_2.1 ;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x116f9c0_0, 0, 32;
    %load/vec4 v0x116fb70_0;
    %cmpi/e 268435471, 0, 32;
    %jmp/0xz  T_2.2, 4;
    %vpi_call 2 23 "$display", "Test 2 Passed" {0 0 0};
    %jmp T_2.3;
T_2.2 ;
    %vpi_call 2 26 "$display", "Test 2 Failed" {0 0 0};
T_2.3 ;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x116f9c0_0, 0, 32;
    %load/vec4 v0x116fb70_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_2.4, 4;
    %vpi_call 2 31 "$display", "Test 3 Passed" {0 0 0};
    %jmp T_2.5;
T_2.4 ;
    %vpi_call 2 34 "$display", "Test 3 Failed" {0 0 0};
T_2.5 ;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
